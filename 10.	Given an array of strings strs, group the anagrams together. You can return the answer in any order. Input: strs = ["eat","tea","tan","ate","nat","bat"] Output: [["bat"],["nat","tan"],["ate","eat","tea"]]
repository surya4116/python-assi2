def group_anagrams(strs):
    anagrams = {}
    for s in strs:
        key = ''.join(sorted(s))
        if key in anagrams:
            anagrams[key].append(s)
        else:
            anagrams[key] = [s]
    return list(anagrams.values())
input_strs = ["eat", "tea", "tan", "ate", "nat", "bat"]
output = group_anagrams(input_strs)
print(output) 
